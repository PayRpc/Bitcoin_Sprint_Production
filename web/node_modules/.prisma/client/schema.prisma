datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ApiKey {
  id        String   @id @default(uuid())
  key       String   @unique
  email     String
  company   String?
  tier      String
  createdAt DateTime @default(now())
  expiresAt DateTime
  revoked   Boolean  @default(false)
}
